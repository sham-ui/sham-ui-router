{
  "name": "sham-ui-router",
  "description": "Router for sham-ui",
  "main": "index.js",
  "author": "Eugeny Burnashov <shamcode@gmail.com>",
  "version": "0.0.10",
  "scripts": {
    "start": "node demo-app/server.js",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "build": "WEBPACK_ENV=build webpack --progress",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.7",
    "eslint": "^5.1.0",
    "eslint-plugin-jest": "^21.18.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^23.4.1",
    "jest-runner-eslint": "^0.6.0",
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0.3",
    "sham-ui-directives": "^1.0.0",
    "sham-ui-jest-preprocessor": "1.0.7",
    "sham-ui-templates": "0.0.18",
    "sham-ui-templates-loader": "1.0.16",
    "sham-ui-test-helpers": "^1.8.0",
    "style-loader": "^0.19.0",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0"
  },
  "dependencies": {
    "navigo": "^7.1.2",
    "sham-ui": "^1.3.5"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties",
      "transform-es2015-modules-umd",
      "transform-object-rest-spread"
    ]
  },
  "jest": {
    "projects": [
      {
        "displayName": "test",
        "transform": {
          "^.+\\.sht$": "sham-ui-jest-preprocessor",
          "^.+\\.js$": "babel-jest"
        },
        "collectCoverageFrom": [
          "src/**/*.js"
        ],
        "testURL": "http://sham-ui-router.example.com"
      },
      {
        "runner": "jest-runner-eslint",
        "displayName": "lint",
        "testMatch": [
          "<rootDir>/src/**/*.js",
          "<rootDir>/__tests__/**/*.js",
          "<rootDir>/__mocks__/**/*.js",
          "<rootDir>/demo-app/**/*.js"
        ]
      }
    ]
  }
}
