{
  "name": "sham-ui-router",
  "description": "Router for sham-ui",
  "main": "index.js",
  "author": "Eugeny Burnashov <shamcode@gmail.com>",
  "version": "0.0.9",
  "scripts": {
    "start": "node demo-app/server.js",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "build": "webpack --mode=production --progress",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "~1.0.0",
    "eslint": "^5.4.0",
    "eslint-plugin-jest": "^21.22.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "jest": "^23.5.0",
    "jest-runner-eslint": "^0.6.0",
    "node-sass": "^4.9.3",
    "sass-loader": "~7.1.0",
    "sham-ui-jest-preprocessor": "1.0.7",
    "sham-ui-templates": "0.0.18",
    "sham-ui-templates-loader": "^1.0.18",
    "sham-ui-test-helpers": "^1.8.0",
    "style-loader": "~0.23.0",
    "webpack": "~4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "~3.2.0",
    "webpack-hot-middleware": "^2.22.3"
  },
  "dependencies": {
    "navigo": "^7.1.1",
    "sham-ui": "^1.3.5",
    "sham-ui-directives": "^1.0.0"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties",
      "transform-es2015-modules-umd",
      "transform-function-bind",
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  },
  "jest": {
    "projects": [
      {
        "displayName": "test",
        "transform": {
          "^.+\\.sht$": "sham-ui-jest-preprocessor",
          "^.+\\.js$": "babel-jest"
        },
        "collectCoverageFrom": [
          "src/**/*.js"
        ],
        "testURL": "http://sham-ui-router.example.com"
      },
      {
        "runner": "jest-runner-eslint",
        "displayName": "lint",
        "testMatch": [
          "<rootDir>/src/**/*.js",
          "<rootDir>/__tests__/**/*.js",
          "<rootDir>/__mocks__/**/*.js",
          "<rootDir>/demo-app/**/*.js"
        ]
      }
    ]
  }
}
